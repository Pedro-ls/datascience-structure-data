class Stack_Queue():    
    
    
    #construtor da classe classe
    def __init__(self, data:list = [], type = "LIFO"):     
        
        "5".isnumeric
        
        self.structure = data
        self.type = type.upper()

    
    # adiciona item a estrutura
    def add(self, value = 0):
        
        self.type = (self.type).upper()
        
        if (self.type == "FIFO"):
            self.structure.append(value)
        elif (self.type == "LIFO"):
            self.structure.insert(0, value)
        
        
    #tirar item na estrutura
    def pop(self):
        
        self.type = (self.type).upper()
        
        if (self.len() > 0):
    
            if (self.type == "FIFO"):
                del self.structure[0]
            
            elif (self.type == "LIFO"):
                del self.structure[0]
            
        else:
    
            if (self.type == "FIFO"):
                print("Queue None")
            elif (self.type == "LIFO"):
                print("Stack None")


    # mostrar estrutura na ordem correta
    def show(self):

        ord = []
        
        for i in reversed(self.structure):

            ord.append(i)
        
        if self.type == "LIFO":
            return ord
        else:
            return self.structure
    
    
    #tamanho da estrutura
    def len(self):
        return len(self.structure)


